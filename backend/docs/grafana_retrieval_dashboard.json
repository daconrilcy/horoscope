{
  "__inputs": [],
  "__requires": [],
  "title": "Retrieval Metrics",
  "editable": true,
  "panels": [
    {
      "type": "graph",
      "title": "Agreement@5",
      "targets": [
        { "expr": "retrieval_agreement_at_5" }
      ]
    },
    {
      "type": "graph",
      "title": "Requests per backend",
      "targets": [
        { "expr": "sum by (backend) (rate(retrieval_requests_total[5m]))" }
      ]
    },
    {
      "type": "graph",
      "title": "Errors per backend/code",
      "targets": [
        { "expr": "sum by (backend,code) (rate(retrieval_errors_total[5m]))" }
      ]
    },
    {
      "type": "graph",
      "title": "Latency per backend",
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(retrieval_latency_seconds_bucket[5m])) by (le,backend))" }
      ]
    }
  ]
}

